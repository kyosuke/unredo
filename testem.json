{
  "framework": "mocha",
  "src_files": [
    "node_modules/expect.js/index.js",
    "unredo.js",
    "test/**/*.js"
  ],
  "launchers": {
    "Node": {
      "command": "npm run coverage",
      "protocol": "tap"
    },
    "coveralls": {
      "command": "npm run coveralls",
      "protocol": "tap"
    }
  },
  "launch_in_dev": [
    "Node",
    "PhantomJS"
  ]
}
