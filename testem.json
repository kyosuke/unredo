{
  "framework": "mocha",
  "src_files": [
    "node_modules/expect.js/index.js",
    "unredo.js",
    "test/**/*.js"
  ],
  "launchers": {
    "Node": {
      "command": "npm run coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js",
      "protocol": "tap"
    }
  },
  "launch_in_dev": [
    "Node",
    "PhantomJS"
  ]
}
